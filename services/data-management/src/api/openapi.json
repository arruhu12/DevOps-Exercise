    "/api/v1/product/store": {
      "post": {
        "tags": [
          "Product Management"
        ],
        "operationId": "storeProduct",
        "summary": "Store Product",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "allOf": [
                  {
                    "$ref": "#/components/schemas/ProductFormRequest"
                  },
                  {
                    "required": [
                      "name",
                      "price"
                    ]
                  }
                ]
              },
              "examples": {
                "Default": {
                  "value": {
                    "name": "TBS Brondolan",
                    "price": "200000"
                  }
                },
                "Missing Price Field": {
                  "value": {
                    "name": "TBS Brondolan"
                  }
                },
                "Missing Name Field": {
                  "value": {
                    "price": "200000"
                  }
                },
                "Missing All Fields": {
                  "value": {}
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/APISuccessResponse"
                },
                "example": {
                  "success": true,
                  "message": "Product Stored Successfully"
                }
              }
            }
          },
          "400": {
            "description": "Input Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "allOf": [
                    {
                      "$ref": "#/components/schemas/APIErrorResponse"
                    },
                    {
                      "$ref": "#/components/schemas/APIValidationError"
                    }
                  ]
                },
                "examples": {
                  "Missing Price Field": {
                    "value": {
                      "success": false,
                      "errorCode": "VALIDATION_ERROR",
                      "message": "Input Validation error",
                      "errors": [
                        {
                          "field": "productPrice",
                          "message": "Product price required"
                        }
                      ]
                    }
                  },
                  "Missing Name Field": {
                    "value": {
                      "success": false,
                      "errorCode": "VALIDATION_ERROR",
                      "message": "Input Validation error",
                      "errors": [
                        {
                          "field": "productName",
                          "message": "Product name required"
                        }
                      ]
                    }
                  },
                  "Missing All Fields": {
                    "value": {
                      "success": false,
                      "errorCode": "VALIDATION_ERROR",
                      "message": "Input Validation error",
                      "errors": [
                        {
                          "field": "productName",
                          "message": "Product name required"
                        },
                        {
                          "field": "productPrice",
                          "message": "Product price required"
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
