{
  "openapi": "3.0.0",
  "info": {
    "title": "Buku Sawit Data Management Service",
    "description": "Service ini dipakai untuk melakukan manajemen data produk, supplier dan karyawan untuk sistem buku sawit.",
    "contact": {
      "email": "it@ninosoftwarehouse.id"
    },
